{
  "name": "math-generator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@math-x-ts/parser": "^0.0.9",
    "@types/katex": "^0.11.1",
    "clsx": "^1.1.1",
    "katex": "^0.15.2",
    "postcss": "^8.4.12",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "tailwindcss": "^3.0.24"
  },
  "scripts": {
    "tailwind:dev": "postcss -w src/vendors/tailwind.css -o src/vendors/tailwind.output.css",
    "tailwind:build": "NODE_ENV=production postcss src/vendors/tailwind.css -o src/vendors/tailwind.output.css",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "start": "npm-run-all -p tailwind:dev react:start",
    "build": "tailwind:build && react:build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "autoprefixer": "^10.2.1",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.17.0",
    "eslint-plugin-react": "^7.22.0",
    "find-imports-ts": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "postcss-cli": "^9.1.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
